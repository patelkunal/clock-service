<configuration>
  <id>clocker-service</id>
  <!-- Display name of the service -->
  <name>clocker-service-0.1.SNAPSHOT</name>
  <!-- Service description -->
  <description>clocker-service-0.1.SNAPSHOT (powered by WinSW)</description>

  <!-- Path to the executable, which should be started -->
  <executable>%JAVA_HOME%\bin\java</executable>
  <arguments>-jar %APP_HOME%\clock-service-0.1.SNAPSHOT.jar</arguments>

  <log mode="roll-by-time" />
</configuration>
